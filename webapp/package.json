{
  "name": "event-calendar-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "latest",
    "gridlex": "2.6.1",
    "isomorphic-fetch": "2.2.1",
    "jquery": "3.3.1",
    "leaflet": "1.3.1",
    "lint-staged": "6.0.1",
    "lodash": "4.17.4",
    "moment": "2.20.1",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "4.1.2",
    "prettier": "1.10.2",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-addons-css-transition-group": "15.6.2",
    "react-big-calendar": "0.18.0",
    "react-custom-scrollbars": "4.2.1",
    "react-dnd": "2.5.4",
    "react-dnd-html5-backend": "2.5.4",
    "react-dom": "16.2.0",
    "react-leaflet": "1.8.2",
    "react-md": "1.2.11",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-router-redux": "4.0.8",
    "react-scripts": "1.1.0",
    "react-social-login": "3.4.2",
    "react-twitter-auth": "0.0.10",
    "recharts": "1.0.0-beta.10",
    "redux": "3.7.2",
    "redux-actions": "2.2.1",
    "redux-batched-actions": "0.2.1",
    "redux-create-action-types": "2.1.0",
    "redux-form": "7.2.1",
    "redux-saga": "0.16.0",
    "redux-thunk": "2.2.0",
    "reselect": "3.0.1",
    "webfontloader": "1.6.28",
    "with-query": "1.0.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "start:prod": "npm-run-all build && serve -s build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "proxy": "http://localhost:5555/api/v1",
  "devDependencies": {
    "eslint-plugin-prettier": "2.5.0",
    "webpack": "^4.1.1"
  }
}
